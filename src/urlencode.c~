#include <stdlib.h>
#include <stdio.h>

void process_char(char a);

int main(int argc, char* argv[]) {
	char buf;
	while ( (buf=getchar()) != EOF) {
		process_char(buf);
	}

	return 0;
}

void process_char(char a) {
	if (a == 32) {
		putchar('+');
		return;
	}

	if (a > 47 && a < 58) {
		putchar(a);
		return;
	}

	if (a > 64 && a < 91) {
		putchar(a);
		return;
	}

	if (a > 96 && a < 123) {
		putchar(a);
		return;
	}

	putchar('%');
	printf("%02X", (unsigned int)a);

	return;

}
